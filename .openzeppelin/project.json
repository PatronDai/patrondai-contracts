{
  "manifestVersion": "2.2",
  "contracts": {
    "PatronDaiCampaignsRegistry": "PatronDaiCampaignsRegistry"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.4.0"
  },
  "name": "squirvels-contracts",
  "version": "1.0.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "200"
      }
    },
    "typechain": {
      "enabled": true,
      "target": "ethers",
      "outDir": "typechain-build"
    },
    "manager": "openzeppelin",
    "solcVersion": "0.5.16",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts"
  },
  "telemetryOptIn": false
}
